		section.shop
			.azir-container
				.page-header.text-center
					h1.h1 Shoping Cart
					.bread-crumb
						a(href="index.html").bread-crumb__link.prv-page Home
						a(href="#").bread-crumb__link.prv-page Pages
						a(href="shop.html").bread-crumb__link.crt-page Shop
				.row.no-gutters
					.col-12
						.shop-cart
							.cart-table
								table
									colgroup
										col(span="1", style="width: 5%")
										col(span="1", style="width: 10%")
										col(span="1", style="width: 40%")
										col(span="1", style="width: 15%")
										col(span="1", style="width: 15%")
										col(span="1", style="width: 15%")
									thead
										tr
											th.product-clear(scope='col') 
											th.product-iamge(scope='col')
											th.product-name(scope='col') Product name
											th.product-price(scope='col') Price
											th.product-quantity(scope='col') Quantity
											th.product-total(scope='col') Total		
									tbody
										tr
											td.product-clear 
												a(href="#")
													i.icon_close
											td.product-image
												.img-wrapper
													img(src="assets/images/pages/shop/small-product1.png", alt="product image")
											td.product-name Away Game
											td.product-price $20.00
											td.product-quantity 
												.quantity-control
													a(href="").control-btn.minus -
													input.quantity(type="text" value="1", readonly="readonly")
													a(href="").control-btn.plus +
											td.product-total $20.00
										tr
											td.product-clear 
												a(href="#")
													i.icon_close
											td.product-image
												.img-wrapper
													img(src="assets/images/pages/shop/small-product2.png", alt="product image")
											td.product-name Kite Runner
											td.product-price $460.00
											td.product-quantity
												.quantity-control
													a(href="").control-btn.minus -
													input.quantity(type="text" value="1", readonly="readonly")
													a(href="").control-btn.plus +
											td.product-total $460.00
										tr
											td.product-clear 
												a(href="#")
													i.icon_close
											td.product-image
												.img-wrapper
													img(src="assets/images/pages/shop/small-product3.png", alt="product image")
											td.product-name Book Firstedition
											td.product-price $26.00
											td.product-quantity
												.quantity-control
													a(href="").control-btn.minus -
													input.quantity(type="text" value="1", readonly="readonly")
													a(href="").control-btn.plus +
											td.product-total $26.00
							.shop-cart_footer
								.row
									.col-12.col-sm-8.col-md-6.col-lg-5
										form.coupon_block(method="GET")
											input.input-form(type="text" placeholder="Coupon code")
											button.btn-pink Apply Coupon
									.col-12.col-sm-4.col-md-6.col-lg-7.text-right
										a.update-cart-btn.btn-pink(href="shop.html") Update Cart
							.shop-cart_total
								.row.justify-content-end
									.col-12.col-sm-6.col-lg-4
										h3.h3 Cart Totals
										table
											colgroup
												col(span="1", style="width: 50%")
												col(span="1", style="width: 50%")
